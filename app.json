{
  "expo": {
    "name": "rifa.mobile",
    "slug": "Rifa Master",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/png/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/png/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/png/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./src/assets/png/favicon.png"
    },
    "experiments": {
      "tsconfigPaths": true
    }
  }
}
